{
	"name": "oym/craft-commerce-product-bundles",
	"description": "Combine multiple products into a purchasable bundle to sell these for a special price.",
	"type": "craft-plugin",
	"require": {
		"php": "^8.0",
		"craftcms/cms": "^4.0",
		"craftcms/commerce": "^4.1"
	},
	"license": "proprietary",
	"authors": [
		{
			"name": "On Your Marks",
			"email": "info@onyourmarks.agency"
		}
	],
	"autoload": {
		"psr-4": {
			"oym\\craft\\commerce\\bundles\\": "src/"
		},
		"files": ["src/deprecated.php"]
	},
	"extra": {
		"name": "Commerce Product Bundles",
		"handle": "commerce-product-bundles",
		"hasCpSettings": true,
		"hasCpSection": false,
		"changelogUrl": "https://raw.githubusercontent.com/onyourmarks-agency/craft-commerce-product-bundles/master/CHANGELOG.md",
		"documentationUrl": "https://github.com/onyourmarks-agency/craft-commerce-product-bundles",
		"components": {
			"productBundleService": "oym\\craft\\commerce\\bundles\\services\\ProductBundleService"
		},
		"class": "oym\\craft\\commerce\\bundles\\Plugin"
	}
}
